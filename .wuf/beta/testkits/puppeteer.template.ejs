import 'regenerator-runtime/runtime';
import { puppeteerUniTestkitFactoryCreator } from 'wix-ui-test-utils/puppeteer';
<%
  components
  .map(component => {
    const driverFactory = component.name + 'Driver';
    const driverPath = component.testkitPath || [
      '../../src/beta',
      component.name,
      component.name + '.uni.driver',
    ].join('\/');

    const factoryName = 'puppeteerUniTestkitFactoryCreator';
    const exportName = component.name + 'Testkit';
%>
import { <%= driverFactory %> } from '<%= driverPath %>';

export const <%= exportName %> = <%= factoryName%>(
  <%= driverFactory %>,
);<% }) %>
